/* pages/huodongguangao/huodongguangao.wxss */
.fail{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #999;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-60%);
}
.fail image{
  width: 424rpx;
  height: 240rpx;
  margin-bottom: 20rpx;
}
.fail text{
  font-size: 32rpx;
}

.title{
  font-size: 40rpx;
  text-align: center;
  padding: 30rpx 0;
  letter-spacing: 2px;
}
.main{
  padding: 0 30rpx;
  line-height: 50rpx;
  letter-spacing: 2px;
  text-indent:2em;
  font-size: 30rpx;
  font-weight: normal;
  color: #333;
}

.bottom{
  float: right;
  text-align: center;
  padding-right: 15rpx;
  font-size: 26rpx;
  color: #333;
  line-height: 50rpx;
  margin-top: 30rpx;
}