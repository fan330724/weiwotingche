/* 支付页面  */

.Cimage{
  padding: 30rpx;
}
.Cimage image{
  width: 100%;
  height: 323rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.total{
  display: flex;
  justify-content: center;
  align-items: flex-end;
  font-size: 90rpx;
  color: #30C77E;
  height: 100rpx;
}
.total span{
  font-size: 24rpx;
  color: #666
}
.total view{height: 100rpx;}
.division{
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 22rpx;
}
.division span{
  width:230rpx;
  height: 1rpx;
  background:#D8D8D8;
}
.division text{
  font-size: 26rpx;
  color: #999;
}
.carPay{
  width: 283rpx;
  padding: 18rpx 0;
  text-align: center;
  font-weight: 700;
  border: 1rpx solid #9D9D9D;
  border-radius: 10rpx;
  margin: 0 auto;
  color: #30C77E;
  margin-top: 28rpx;
}
.parkName{
  text-align: center;
  font-size: 34rpx;
  color: #333;
  line-height: 48rpx;
  font-weight: 700; 
  width: 74%;
  margin: 0 auto;
  margin-top: 38rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;

}
.time {
  margin-top: 28rpx;
}
.time view{
  color: #666666;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  
}
.bt-pay{
  width: 60%;
  height: 72rpx;
  line-height: 72rpx;
  margin:0 auto; 
  font-size: 34rpx;
  text-align: center;
  background-color: #00d386;
  color: #fff;
  border-radius: 18px;
  margin-top: 40rpx;
  box-shadow: 0rpx 0rpx 4rpx #c0c0c0
}

.back{
  font-size: 34rpx;
  color: #30C77E;
  width: 60%;
  height: 72rpx;
  line-height: 72rpx;
  margin:0 auto; 
  text-align: center;
  background-color: #efefef;
  border-radius: 18px;
