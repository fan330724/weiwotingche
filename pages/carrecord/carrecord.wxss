/* pages/carrecord/carrecord.wxss */
page {
  font-size: 14px;
  background: #f6f6f6
}

.widgets__item {
  margin: 20rpx 20rpx;
  background-color: #fff;
  padding: 22rpx 32rpx;
}

.nomore {
  text-align: center;
  color: #999;
  margin-top: 40rpx;
}

.total {
  margin-top: 18rpx;
  color: #333;
  font-family: "微软雅黑";
  height: 44rpx;
  line-height: 44rpx;
  display: flex;
  align-items: center;
  align-content: center;
}

.pay {
  margin-top: 18rpx;
  color: #333;
  font-family: "微软雅黑";
  height: 44rpx;
}

.time {
  margin-top: 18rpx;
  color: #333;
  font-family: "微软雅黑";
  height: 44rpx;
  font-size: 28rpx;
  color: #bfbfbf;
}
.red{
  color: #F46262
}
.left {
  float: left;
  color: #bfbfbf;
}

.name {
  width: 380rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 32rpx;
  color: #666666;
}
.totals{
  font-size: 50rpx;
  color: #666666;
}
.bTime {
  width: 380rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  color: #999
}
.dw{
  color: #bfbfbf;
  line-height: 50rpx;
}
.right {
  float: right;
}

.row {
  display: inline-block;
  width: 14rpx;
  height: 14rpx;
  border-radius: 14rpx;
  margin-right: 20rpx;
}

.yellow {
  background-color: orange;
}

.green {
  background-color: green;
}

.title_p{
  margin: 20rpx;
  font-size: 30rpx;
  color: #999;
  height: 42rpx;
  line-height: 42rpx;
}

