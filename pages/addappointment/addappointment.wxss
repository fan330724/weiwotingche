page{
  position: relative;
  height: 100%
}
/* 修改设备名称 */
.modu{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 999999
}
.modu_box{
  position: absolute;
  left: 50%;
  margin-left: -290rpx;
  width: 580rpx;
  top: 120rpx;
  height: 330rpx;
  background: #fff;
  overflow: hidden;
  border-radius: 10rpx;
}
.box_header{text-align: center;color: #fff;font-size: 32rpx;background: #30C77E;padding: 8rpx 0;}
.modu_box .canName{margin-top: 80rpx;}
.box_btn{
  margin-top: 60rpx;
  display: flex;
  justify-content: space-around;
}
.box_btn view{font-size: 32rpx;}
.box_btn view:nth-child(1){color: rgb(241, 72, 6)}
.box_btn view:nth-child(2){color: #30C77E}
.myment{
  max-height: 472rpx;
  box-shadow: 0 2rpx 15rpx #c0c0c0;
}
.i_ment{
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: nowrap;
  padding: 40rpx 0;
}

.i_ment .btn view{
  display: inline-block;
  margin: 0 14rpx;
  font-size: 38rpx;

}
.i_ment .btn view:nth-child(1){
  color: rgb(71, 186, 231)
}
.i_ment .btn view:nth-child(2){
  color: rgb(223, 139, 43)
}
.i_ment image:nth-child(1){
  width: 78rpx;
  height: 63rpx;
}
.msg {
  font-size: 26rpx;
  color: #c0c0c0;
  text-align: center;
  width: 100%;
  padding: 16rpx 0; 
}
.addment .header{
  margin-top: 60rpx;
  color: #30C77E;
  text-align: center;
  font-size: 34rpx;
  font-weight: 700;
}
.ico_box{
  text-align: center;
  margin-top: 30rpx;
}
.ico_box .ico{
  display: inline-block;
  font-size: 164rpx;
  color: #30C77E;
}
.scan{
  text-align: center;
  width: 55%;
  margin:0 auto;
  border-bottom: 1rpx solid #30C77E; 
  font-size: 28rpx;
  margin-top: 34rpx;
}
.scanbtn{
  margin: 0 auto;
  width: 55%; 
  padding: 12rpx 0;
  text-align: center;
  color: #fff;
  background-color: #30C77E;
  border-radius: 10rpx;
  box-shadow: 0 0 30rpx #30C77E;
  margin-top: 100rpx;
}
.re_name{
  width: 286rpx;
}



