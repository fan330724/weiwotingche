/* pages/my/my.wxss */
.bg{
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  width: 100%;
  height: 390rpx;
  display: flex;
  flex-direction: column;
}
.userinfo{
  padding: 50rpx 30rpx;
  display: flex;
  align-items: center;
}
.userinfo image{
  width: 122rpx;
  height: 122rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}
.userinfo .conten{
  color: white;
}
.userinfo .conten .userinfo-nickname{
  margin-bottom: 15rpx;
  letter-spacing: 2px;
}
.userinfo .conten .userinfo-nickname text{
  font-size: 24rpx;
}
.userinfo .conten .hui{
  width: 104rpx;
  height: 36rpx;
  background: #22B16D;
  border-radius: 8px;
  text-align: center;
  line-height: 36rpx;
  font-size: 16rpx;
}
.userinfo .conten .hui1{
  width: 104rpx;
  height: 36rpx;
  border-radius: 0;
}
.main{
  display: flex;
  align-items: center;
  justify-content: space-around;
  color: white;
  font-size: 28rpx;
  width: 500rpx;
  margin: 0 auto;
}
.main text{
  color: #F9F578;
  font-size: 54rpx;
}
.main .bor{
  display: flex;
  flex-direction: column;
  align-items: center;
  letter-spacing: 1px;
  justify-content: center;
}
.main image{
  width: 56rpx;
  height: 8rpx;
}
.main .shugang{
  width: 2rpx;
  height: 46rpx;
  background: #e2e2e2;
  border-radius: 100px;
}
.bann{
  width: 690rpx;
  height: 104rpx;
  margin: 20rpx auto;
}
.bann image{
  width: 100%;
  height: 100%;
}
.list{
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin: 50rpx 0;
}
.list .list-l{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.list .list-l image{
  width: 60rpx;
  height: 60rpx;
}
.list .list-l text{
  font-size: 28rpx;
  color: #5B5E63;
  letter-spacing: 2px;
}
.set{
  padding: 0 30rpx;
  margin-top: 35rpx;
}
.set .title{
  font-size: 32rpx;
  letter-spacing: 2px;
  margin-bottom: 25rpx;
}
.set .lis{
  display: flex;
  justify-content: space-between;
  padding: 30rpx 0;
}
.set .lis view{
  display: flex;
  align-items: center;
}
.set .lis image{
  width: 40rpx;
  height: 40rpx;
}
.set .lis text{
  margin-left: 32rpx;
  color: #3D444D;
  font-size: 32rpx;
  letter-spacing: 1px;
}
.set .lis .icon text{
  font-size: 40rpx;
  font-weight: 600;
}

/* 弹窗登录 */
.modu{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 999999
}
.modu .L_box .login {
  font-size: 14px;
  border-radius: 0rpx;
  margin: 20rpx 0;
  width: 90%;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 10rpx;
  text-align: center;
  color: white;
  background: #30C77E;
  border: 1px solid #30C77E;
  margin: 0 auto;
  margin-top: 30rpx;
}

.modu .L_box {
  width: 600rpx;
  position: absolute;
  left: 50%;
  margin-left: -300rpx;
  top: 250rpx;
  border-radius: 20rpx;
  overflow: hidden;
  background: #fff;
  z-index: 999999;
}

.modu .L_box .l_logo {
  width: 600rpx;
  height: 300rpx;
  background: #30C77E;
  text-align: center;
}

.modu .L_box .l_logo image {
  margin-top: 50rpx;
  width: 300rpx;
  height: 200rpx;
}

.text {
  background: #fff;
  text-align: center;
  padding: 20rpx 0;
}

.text>view:nth-child(1) {
  font-size: 14px;
}

.text>view:nth-child(2) {
  font-size: 12px;
  color: #c0c0c0
}

.modu .L_box button {
  font-size: 14px;
  border-radius: 0rpx;
  margin: 20rpx 0;
  width: 90%;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 10rpx;
  text-align: center;
  color: #30C77E;
  border: 1px solid #30C77E;
  margin: 0 auto;
  margin-top: 30rpx;
}

.modu .L_box button::after {
  width: 0;
  height: 0
}

.clear {
  width: 90%;
  font-size: 14px;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 10rpx;
  text-align: center;
  color: #30C77E;
  border: 1px solid #30C77E;
  margin: 0 auto;
  margin-top: 30rpx;
  margin-bottom: 50rpx;
}

.shelf{
  background: white;
  border-radius: 10px;
  padding: 30rpx;
  color: #282828;
  font-size: 32rpx;
  width: 320rpx;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}