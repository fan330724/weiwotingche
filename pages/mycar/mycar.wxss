/* pages/mycar/mycar.wxss */

page {
  height: 100%;
  background: #fff
}

/* 车牌新样式 */
.ts{
  font-size: 26rpx;
  margin-top: 40rpx;
  color: #BEBEBE;
  padding-left: 40rpx;
}

.card {
  
  width: 630rpx;
  height: 234rpx;
  margin: 0 auto;
  line-height: 218rpx;
  text-align: center;
  color: #fff;
  font-size: 76rpx;
}

.Ycard {
  background: url(http://124.70.23.12:8084/wxx/images/card.png) no-repeat center;
  background-size: 100%;
  position: relative;
  margin-top: 40rpx;
}

.Ycard icon {
  position: absolute;
  bottom: 32rpx;
  right: 42rpx;
  width: 80rpx;
  line-height: 80rpx;
  height: 80rpx;
  font-size: 24px;
}

.Ncard {
  margin-top: 40rpx;
  background: url(http://124.70.23.12:8084/wxx/images/ncard.png) no-repeat center;
  background-size: 100%;
}
