/* keyboard.wxss */

/* 车牌新样式 */
.carID{
  display: flex;
  justify-content: space-around;
  align-content: center;
  width: 90%;
  margin: 0 auto;
  margin-bottom: 44rpx;
}
.carList {
  width: 74rpx;
  height: 70rpx;
  border: 1px solid #999999;
  border-radius:12rpx;
}
.addCard{
  background-color: #30C77E;
  border: 1px solid #30C77E
}
.carList view{
  width: 54rpx;
  height: 50rpx;
  margin: 0 auto;
  text-align: center;
  line-height: 50rpx;
  margin-top: 8rpx;
}
.addCard view{
  color: #fff;
  border: 2rpx solid #fff;
  border-radius: 8rpx;
}










image{width:750rpx;height: 130px}
.page {
  position: absolute;
  width: 100%;
  height: 100%;
  /* background: url(http://longmen-1251225286.costj.myqcloud.com/bg.png) top center; */
  background-size: 100% 100%;
  background-color: #fff
}

.title {
  width: 100%;
  margin-top: 98rpx;
  height: 280rpx;
  text-align: center;
  margin-bottom: 68rpx
}

.title-img {
  text-align: center;
}

.text {
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 90%;
  height: 90rpx;
  line-height: 100rpx;
  margin: 20rpx auto;
  color: #000;
  background-color: #fff;
  font-size: 36rpx;
  border-radius: 20rpx
}

.text_con {
  border: 1rpx solid #c0c0c0;
}

.text_con_focus {
  width: 85%;
  padding: 0 20rpx;
  border: 1rpx solid green;
  border-radius: 20rpx;
  letter-spacing:8rpx;
  display: flex;                                                                                                                                
  justify-content: center;                                                                                                                                             
}

.search_icon {
  width: 50rpx;
  height: 50rpx;
  margin: 0 20rpx;
}

.photo_icon{
  position: absolute;
  top:400rpx;
  right: 50rpx;
  width: 50rpx;
  height: 50rpx;
  margin: 0 20rpx;
}

.del_icon {
  width: 100%;
  height: 100%;
  background: url(http://longmen-1251225286.costj.myqcloud.com/WechatIMG2.png) top center;
  background-size: 100% 100%;
}

.placeholder {
  color: #bdc3c7;
  margin-left: 40rpx
}

.keyboard {
  z-index: 9999;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: auto;
  background: #eaf1f9;
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  border-bottom: 15rpx solid #eaf1f9;
  border-top: 20rpx solid #eaf1f9;
}

.td {
  font-family: "微软雅黑";
  flex-grow: 1;
  text-align: center;
  font-size: 36rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  margin: 5rpx;
  border: 1rpx solid #cdd0d5;
  color: #1e1e1e;
  border-radius: 10rpx 10rpx 10rpx 10rpx;
  box-shadow: 5rpx 5rpx 2rpx #cdd0d5;
}

.td_nor {
  flex: 1 1 10%;
}

.td_num {
  flex: 1 1 8%;
}

.td_spec_fin {
  background-color: #30C77E;
  color: #fff;
  flex: 1 1 23%;
}
.td_spec_del{
  width: 50px;
  height: 50px;
}

.board_bg {
  box-shadow: 0 0 0 #e5e5e5;
  background: #e5e5e5;
}
.board_bg_spec {
  box-shadow: 0 0 0 #e5e5e5;
}

.parking {
  position: absolute;
  right: 20rpx;
  bottom: 100rpx;
}

.parking_icon {
  width: 200rpx;
  height: 200rpx;
}

.warn {
  width: 100%;
  height: 20%;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-family: '宋体';
  font-size: 28rpx;
  color: #8b9397;
  text-align: center;
  line-height: 48rpx;
}

.warn_text {
  width: 80%;
}

.bottom {
  position: absolute;
  bottom: 30rpx;
  width: 100%;
  font-size: 26rpx;
  font-family: '宋体';
  color: green;
  line-height: 36rpx;
}

.tel {
  justify-content: center;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.phone_icon {
  width: 22rpx;
  height: 22rpx;
  margin: 0 10rpx;
}
.msage{font-size: 30rpx;text-align: center;color: #333;margin-bottom: 40rpx;}
.newcar{ 
  width: 85%;
  margin: 0 auto;
  font-size: 14px;
  margin-bottom: 40rpx;
  display: flex;
  justify-content: flex-end;
}
.newcar image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.newcar .txt{font-size: 14px;height:40rpx;line-height: 40rpx;color:#30C77E;} 
.newcar switch{float:right;} 

.btn{
  width: 602rpx;
  height: 90rpx;
  background-color: #30C77E;
  border-radius:100rpx;
  color: #fff;
  text-align: center;
  line-height: 90rpx;
  margin: 0 auto;
  box-shadow: rgba(113, 231, 174,.5) 0 16rpx 60rpx 0rpx;
}