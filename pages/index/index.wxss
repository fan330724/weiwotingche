/* 弹出框 */
.alert-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.3;
}

.pop .modalDlg {
  width: 80%;
  /* height: 55%; */
  position: fixed;
  top: 45%;
  left: -1%;
  z-index: 9999;
  box-sizing: border-box;
  padding: 25rpx;
  margin: -370rpx 85rpx;
  border-radius: 18rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
}

.pop image {
  width: 100%;
  height: 100%;
}

.pop .receive {
  position: relative;
  width: 608rpx;
  height: 800rpx;
  z-index: 999999;
}
.pop .accbtn{
  position: absolute;
  bottom: 50rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 280rpx;
  height: 82rpx;
  background: url('http://124.70.23.12:8084/gameIcon/image/lingqujilu.png') no-repeat;
  background-size: 100% 100%;
  padding: 0;
}
button::after {
  border: none;
}
.pop .close {
  width: 60rpx;
  height: 60rpx;
  margin-top: 30rpx;
}

.pop .rece {
  position: absolute;
  left: 50%;
  bottom: 25rpx;
  transform: translateX(-50%);
  width: 280rpx;
  height: 80rpx;
  background: #242329;
  border-radius: 44rpx;
  line-height: 80rpx;
  text-align: center;
  color: white;
  font-size: 32rpx;
  letter-spacing: 2rpx;
}
.pop .modalDlg1{
  height: 60%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
}
.pop .receivee{  
            
}
.pop .receivee image:nth-of-type(1){
  width: 552rpx;
  height: 658rpx;
}
.pop .receivee image:nth-of-type(2){
  width: 180rpx;
  height: 180rpx;
  position: absolute;
  top: 460rpx;
  left: 50%;
  margin: -90rpx;
}
.pop .receivee image.scale{
  animation: scale 1s infinite;
  transform-origin: center;
  transition: all 1s;
}
.pop .receivee image.rotate{
  animation: rotate 2s infinite;
}
@keyframes rotate{
  0%{
    transform: rotateY(0deg);
  }
  100%{
    transform: rotateY(360deg);
  }
}
@keyframes scale{
  0%{
    transform: scale(.8);
  }
  50%{
    transform: scale(1);
  }
  100%{
    transform: scale(.8);
  }
}
.pop .receive1{
  background: url('http://124.70.23.12:8084/gameIcon/image/bt-success.png') no-repeat;
  background-size: 100% 100%;
  width: 552rpx;
  height: 658rpx;
  position: relative;
}
.pop .receive1 .top,.pop .receive1 .price,.pop .receive1 .bottom{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  color: white;
}
.pop .receive1 .top{
  top: 200rpx;
  font-size: 46rpx;
}
.pop .receive1 .price{
  top: 290rpx;
  font-size: 85rpx;
}
.pop .receive1 .bottom{
  top: 420rpx;
  font-size: 24rpx
}
.pop .butt{
  position: absolute;
  bottom: 9%;
  left: 50%;
  transform: translateX(-50%);
  width: 340rpx;
  height: 76rpx;
}

