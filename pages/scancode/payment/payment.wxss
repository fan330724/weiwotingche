/* pages/scancode/scancode.wxss */
view {
  color: #333;
}

.fixedImg {
  position: fixed;
  left: 50%;
  bottom: 26rpx;
  transform: translateX(-50%);
  width: 266rpx;
  height: 140rpx;
}


/* 支付页面  */
.p-m{
  margin-top: 50px;
}
.total {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  font-size: 90rpx;
  color: #30C77E;
  height: 100rpx;
}

.total span {
  font-size: 24rpx;
  color: #666
}

.total view {
  height: 100rpx;
}

.division {
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 22rpx;
}

.division span {
  width: 230rpx;
  height: 1rpx;
  background: #D8D8D8;
}

.division text {
  font-size: 26rpx;
  color: #999;
}

.carPay {
  width: 283rpx;
  padding: 18rpx 0;
  text-align: center;
  font-weight: 700;
  border: 1rpx solid #9D9D9D;
  border-radius: 10rpx;
  margin: 0 auto;
  color: #30C77E;
  margin-top: 28rpx;
}

.parkName {
  text-align: center;
  font-size: 34rpx;
  color: #333;
  line-height: 48rpx;
  font-weight: 700;
  width: 74%;
  margin: 0 auto;
  margin-top: 38rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;

}

.time {
  margin-top: 28rpx;
}

.time view {
  color: #666666;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;

}
.sercht{
  width: 75%;
  margin: 0 auto;
  height: 90rpx;
  background-color: #30C77E;
  border-radius: 10px;
  color: #fff;
  text-align: center;
  line-height: 90rpx;
  font-size: 16px;
  margin-top: 100rpx;

}

.sucess,
.err {
  padding-top: 90rpx
}

.s-title {
  text-align: center;
  color: #00925d;
  margin-bottom: 80rpx;
  font-size: 44rpx
}

.toindex {
  width: 74%;
  height: 90rpx;
  line-height: 90rpx;
  background: #00925d;
  border-radius: 18px;
  text-align: center;
  margin: 0 auto;
  color: #fff;
  margin-top: 110rpx
}