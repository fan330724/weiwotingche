/* pages/wechatactivities/welfare/welfare.wxss */
page{
  height: 100vh;
}
.bg{
  width: 750rpx;
  height: 100%;
  position: relative;
}
.butt{
  position: absolute;
  bottom: 20rpx;
  left: 50%;
  transform: translateX(-50%);
}
.butt image{
  width: 428rpx;
  height: 110rpx;
  margin-bottom: 24rpx;
}

.pop{
  width: 750rpx;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.pop image{
  width: 100%;
  height: 100%;
}
.pop .close{
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 60rpx;
  height: 60rpx;
}

.l11{
  width: 38%;
  height: 21%;
  position: absolute;
  top: 41%;
  left: 50%;
  transform: translateX(-50%);
  /* background: blue; */
}