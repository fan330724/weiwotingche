/*车牌样式  */

.text {
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 90%;
  height: 68rpx;
  line-height: 100rpx;
  margin: 20rpx auto;
  color: #000;
  background-color: #fff;
  font-size: 36rpx;
  border-radius: 20rpx;
}

.text_con {
  border: 1rpx solid #c0c0c0;
}

.text_con_focus {
  width: 85%;
  padding: 0 20rpx;
  border: 1rpx solid #00925d;
  border-radius: 20rpx;
  letter-spacing: 8rpx;
}

.search_icon {
  width: 50rpx;
  height: 50rpx;
  margin: 0 20rpx;
}

.photo_icon {
  position: absolute;
  top: 400rpx;
  right: 50rpx;
  width: 50rpx;
  height: 50rpx;
  margin: 0 20rpx;
}

.del_icon {
  width: 100%;
  height: 100%;
  background: url(http://longmen-1251225286.costj.myqcloud.com/WechatIMG2.png) top center;
  background-size: 100% 100%;
}

.placeholder {
  color: #bdc3c7;
  margin-left: 40rpx;
}

.keyboard {
  z-index: 9999;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: auto;
  background: #eaf1f9;
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  border-bottom: 15rpx solid #eaf1f9;
  border-top: 20rpx solid #eaf1f9;
}

.td {
  font-family: "微软雅黑";
  flex-grow: 1;
  text-align: center;
  font-size: 36rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  margin: 5rpx;
  border: 1rpx solid #cdd0d5;
  color: #1e1e1e;
  border-radius: 10rpx 10rpx 10rpx 10rpx;
  box-shadow: 5rpx 5rpx 2rpx #cdd0d5;
}

.td_nor {
  flex: 1 1 10%;
}

.td_num {
  flex: 1 1 8%;
}

.td_spec_fin {
  background-color: #ff7149;
  color: #fff;
  flex: 1 1 23%;
}

.td_spec_del {
  width: 50px;
  height: 50px;
}

.board_bg {
  box-shadow: 0 0 0 #e5e5e5;
  background: #e5e5e5;
}

.board_bg_spec {
  box-shadow: 0 0 0 #e5e5e5;
}

.parking {
  position: absolute;
  right: 20rpx;
  bottom: 100rpx;
}

.parking_icon {
  width: 200rpx;
  height: 200rpx;
}

.warn {
  width: 100%;
  height: 20%;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-family: '宋体';
  font-size: 28rpx;
  color: #8b9397;
  text-align: center;
  line-height: 48rpx;
}

.warn_text {
  width: 80%;
}

.bottom {
  position: absolute;
  bottom: 30rpx;
  width: 100%;
  font-size: 26rpx;
  font-family: '宋体';
  color: #00925d;
  line-height: 36rpx;
}

.tel {
  justify-content: center;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.phone_icon {
  width: 22rpx;
  height: 22rpx;
  margin: 0 10rpx;
}

.newcar {
  width: 85%;
  margin: 0 auto;
}

.newcar text {
  font-size: 14px;
  color: #00925d;
}

.newcar switch {
  float: right;
}

.section1 {
  margin: 0 auto;
  border: 1px solid #c0c0c0;
  border-radius: 10px;
  padding: 0 20rpx;
  font-size: 28rpx;
  height: 68rpx;
  line-height: 68rpx;
  width: 70%;
}

.btn {
  width: 320rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 80rpx auto;
  text-align: center;
  background-color: #30C77E ;
  color: #fff;
  border-radius: 10px;
  font-size: 16px;
}

.carn {
  width: 80%;
  margin: 40rpx auto;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

/*模态框  */

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 540rpx;
  overflow: hidden;
  position: fixed;
  top: 18%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin-left:  105rpx;
  border-radius: 36rpx;
}

.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}

.modal-content {
  padding: 50rpx 32rpx;
}

.modal-input {
  display: flex;
  background: #fff;
  border: 2rpx solid #ddd;
  border-radius: 4rpx;
  font-size: 28rpx;
}

.input {
  width: 100%;
  height: 82rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  color: #333;
}

input-holder {
  color: #666;
  font-size: 28rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-cancel {
  width: 100%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}

.btn-confirm {
  width: 100%;
  color: #00925d;
  text-align: center;
}


