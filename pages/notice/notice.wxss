/* pages/notice/notice.wxss */
view{
  letter-spacing: 1px;
  font-size: 24rpx;
}
.fail{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #999;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-60%);
}
.fail image{
  width: 424rpx;
  height: 240rpx;
  margin-bottom: 20rpx;
}
.fail text{
  font-size: 32rpx;
}
.bg{
  background: #F2F7F8;
  padding: 30rpx 30rpx;
}
.list{
  padding: 20rpx 30rpx;
  background: white;
  margin-bottom: 2px;
  border-radius: 5px;
}
.list .title{
  color: #000;
  font-size: 30rpx;
  font-weight: bold;
}
.list .price{
  display: flex;
  align-items: center;
  margin: 10rpx 0;
}
.list .circular{
  width: 10rpx;
  height: 10rpx;
  background: #30C77E;
  border-radius: 50%;
  margin-right: 10rpx;
}
.list .park{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.list .park view:nth-of-type(1){
  display: flex;
  align-items: center;
}
.list .park .date{
  color: #979797;
}