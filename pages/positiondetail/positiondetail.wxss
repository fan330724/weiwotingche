/* pages/positiondetail/positiondetail.wxss */
.content{
  padding: 0 30rpx;
}
.content .list{
  padding: 30rpx;
  border-bottom: 1px dashed #ccc;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.content .list .t-l{
  display: flex;
  /* align-items: center; */
}
.content .list .t-l icon{
  font-size: 50rpx;
  color: #30C87F;
}
.content .list .left{
  margin-right: 20rpx;
}
.content .list .center{
  color: #3D444D;
  font-size: 32rpx;
  padding-right: 10rpx;
}
.content .list .t-l image{
  width: 50rpx;
  height: 50rpx;
}
.content .list .right image{
  width: 50rpx;
  height: 50rpx;
}
.img{
  display: flex;
}
.img image{
  width: 200rpx;
  height: 200rpx;
  margin-top: 30rpx;
}
.img image:nth-of-type(2){
  margin-left: 45rpx;
  margin-right: 45rpx;
}